{
    "name": "starkeen/google-search-api",
    "description": "Library for Google Search API",
    "keywords": ["google", "api", "search", "client", "HTTP client"],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Andrey Panisko",
            "email": "starkeen@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.8",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "phpunit/php-code-coverage": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "GoogleSearchApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GoogleSearchApi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit tests",
        "test-coverage": "phpunit --coverage-html=coverage tests"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch": true
    }
}
